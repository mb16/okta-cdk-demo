{
  "//": {
    "metadata": {
      "backend": "cloud",
      "cloud": "tfc",
      "stackName": "okta-cdk-demo",
      "version": "0.15.5"
    },
    "outputs": {
    }
  },
  "module": {
    "applications": {
      "//": {
        "metadata": {
          "path": "okta-cdk-demo/applications",
          "uniqueId": "applications"
        }
      },
      "source": "./assets/local-module-applications/8AA532E7DBDABCC5BABFD75FA6D0200C"
    },
    "auth-servers": {
      "//": {
        "metadata": {
          "path": "okta-cdk-demo/auth-servers",
          "uniqueId": "auth-servers"
        }
      },
      "Customer_Portal_App_Id": "${module.applications.customer_portal_app_id}",
      "OIDC_Client_Id": "${module.applications.oidc_superwidget_id}",
      "source": "./assets/local-module-auth-servers/022F8ADAC96AD979CFEFDC884B054868"
    }
  },
  "provider": {
    "okta": [
      {
        "api_token": "${var.api_token}",
        "base_url": "${var.base_url}",
        "org_name": "${var.org_name}"
      }
    ]
  },
  "resource": {
    "okta_admin_role_custom": {
      "app_admin": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/app_admin",
            "uniqueId": "app_admin"
          }
        },
        "description": "This role users to manage applications",
        "label": "App Admin Role",
        "permissions": [
          "okta.apps.manage"
        ]
      }
    },
    "okta_authenticator": {
      "email": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/email",
            "uniqueId": "email"
          }
        },
        "key": "okta_email",
        "name": "Email",
        "settings": "${jsonencode (\n\t\t{\n\t\t\t\"allowedFor\": \"any\"\n\t\t\t\"tokenLifetimeInMinutes\" : 5\n\t\t}\n\t)}"
      },
      "webauthn": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/webauthn",
            "uniqueId": "webauthn"
          }
        },
        "key": "webauthn",
        "name": "Web AuthN"
      }
    },
    "okta_group": {
      "dmzadmin_group": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/dmzadmin_group",
            "uniqueId": "dmzadmin_group"
          }
        },
        "description": "Group that contains employees that are responsible for DMZ Servers",
        "name": "DMZ Server Admins"
      },
      "service_accounts": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/service_accounts",
            "uniqueId": "service_accounts"
          }
        },
        "description": "Group that contains all users that represent Service Accounts",
        "name": "Okta Service Accounts"
      },
      "test_group": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/test_group",
            "uniqueId": "test_group"
          }
        },
        "description": "For testing and demonstration",
        "name": "Renamed Test Group"
      }
    },
    "okta_group_rule": {
      "dmzadmin_grouprule": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/dmzadmin_grouprule",
            "uniqueId": "dmzadmin_grouprule"
          }
        },
        "expression_type": "urn:okta:expression:1.0",
        "expression_value": "user.department==\"DMZ Ops\"",
        "group_assignments": [
          "${resource.okta_group.dmzadmin_group.id}"
        ],
        "name": "DMZ Admins",
        "status": "ACTIVE"
      },
      "test_rule": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/test_rule",
            "uniqueId": "test_rule"
          }
        },
        "expression_type": "urn:okta:expression:1.0",
        "expression_value": "String.startsWith(user.firstName,\"somename\")",
        "group_assignments": [
          "${okta_group.test_group.id}"
        ],
        "name": "Test Rule",
        "status": "ACTIVE"
      }
    },
    "okta_inline_hook": {
      "validate_client_id_registration": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/validate_client_id_registration",
            "uniqueId": "validate_client_id_registration"
          }
        },
        "auth": {
          "key": "Authorization",
          "type": "HEADER",
          "value": "1234"
        },
        "channel": {
          "method": "POST",
          "type": "HTTP",
          "uri": "https://iatcoreapim.azure-api.net/okta/registration",
          "version": "1.0.0"
        },
        "name": "Validate Client ID on Registration",
        "status": "ACTIVE",
        "type": "com.okta.user.pre-registration",
        "version": "1.0.0"
      }
    },
    "okta_network_zone": {
      "blacklisted_countries": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/blacklisted_countries",
            "uniqueId": "blacklisted_countries"
          }
        },
        "dynamic_locations": [
          "AF",
          "IR",
          "KP"
        ],
        "name": "Blacklisted Countries",
        "type": "DYNAMIC"
      },
      "corporate_locations": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/corporate_locations",
            "uniqueId": "corporate_locations"
          }
        },
        "dynamic_locations": [
          "BS",
          "JM"
        ],
        "name": "Corporate Locations",
        "type": "DYNAMIC"
      },
      "tor_blocker": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/tor_blocker",
            "uniqueId": "tor_blocker"
          }
        },
        "dynamic_proxy_type": "TorAnonymizer",
        "name": "TOR Blocker",
        "type": "DYNAMIC",
        "usage": "BLOCKLIST"
      }
    },
    "okta_trusted_origin": {
      "customer_portal_local": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/customer_portal_local",
            "uniqueId": "customer_portal_local"
          }
        },
        "name": "customerportal.local",
        "origin": "https://localhost:44369/",
        "scopes": [
          "CORS"
        ]
      },
      "super_widget": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/super_widget",
            "uniqueId": "super_widget"
          }
        },
        "name": "super-widget",
        "origin": "https://super-widget.oktaprise.com",
        "scopes": [
          "CORS",
          "REDIRECT"
        ]
      }
    },
    "okta_user": {
      "okta_terraform_admin": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/okta_terraform_admin",
            "uniqueId": "okta_terraform_admin"
          }
        },
        "department": "DMZ Ops",
        "email": "OktaTerraformAdmin@iatcore.com",
        "first_name": "Okta",
        "last_name": "Terraform Admin",
        "login": "OktaTerraformAdmin@iatcore.com",
        "status": "ACTIVE"
      }
    },
    "okta_user_schema_property": {
      "clientId": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/clientId",
            "uniqueId": "clientId"
          }
        },
        "description": "Stores the customer's account ID",
        "index": "clientId",
        "master": "OKTA",
        "permissions": "READ_WRITE",
        "scope": "SELF",
        "title": "Client ID",
        "type": "string"
      },
      "crn_extension": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/crn_extension",
            "uniqueId": "crn_extension"
          }
        },
        "index": "customer_reference_number",
        "master": "PROFILE_MASTER",
        "required": false,
        "title": "Customer Reference Number",
        "type": "string"
      },
      "isVerified": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/isVerified",
            "uniqueId": "isVerified"
          }
        },
        "description": "Stores whether the user has been verified as a customer",
        "index": "isVerified",
        "master": "OKTA",
        "permissions": "READ_WRITE",
        "scope": "SELF",
        "title": "Is Verified?",
        "type": "boolean"
      }
    }
  },
  "terraform": {
    "cloud": {
      "hostname": "app.terraform.io",
      "organization": "dev-05844969-okta-com-DEV",
      "workspaces": {
        "name": "okta-terraform-ref-dev-env"
      }
    },
    "required_providers": {
      "okta": {
        "source": "okta/okta",
        "version": "3.42.0"
      }
    }
  },
  "variable": {
    "api_token": {
    },
    "base_url": {
    },
    "org_name": {
    }
  }
}