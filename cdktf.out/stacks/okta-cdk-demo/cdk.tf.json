{
  "//": {
    "metadata": {
      "backend": "cloud",
      "cloud": "tfc",
      "stackName": "okta-cdk-demo",
      "version": "0.15.5"
    },
    "outputs": {
    }
  },
  "provider": {
    "okta": [
      {
        "api_token": "${var.api_token}",
        "base_url": "${var.base_url}",
        "org_name": "${var.org_name}"
      }
    ]
  },
  "resource": {
    "okta_app_oauth": {
      "MichaelApp": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/MichaelApp",
            "uniqueId": "MichaelApp"
          }
        },
        "grant_types": [
          "authorization_code"
        ],
        "label": "Michael App",
        "omit_secret": false,
        "redirect_uris": [
          "https://localhost:5001/signin-oidc"
        ],
        "response_types": [
          "code"
        ],
        "type": "web"
      }
    },
    "okta_group": {
      "michaels_group": {
        "//": {
          "metadata": {
            "path": "okta-cdk-demo/michaels_group",
            "uniqueId": "michaels_group"
          }
        },
        "description": "Group to verify CDK",
        "name": "Michael's Group"
      }
    }
  },
  "terraform": {
    "cloud": {
      "hostname": "app.terraform.io",
      "organization": "dev-05844969-okta-com-DEV",
      "workspaces": {
        "name": "okta-cdk-demo"
      }
    },
    "required_providers": {
      "okta": {
        "source": "okta/okta",
        "version": "3.42.0"
      }
    }
  },
  "variable": {
    "api_token": {
    },
    "base_url": {
    },
    "org_name": {
    }
  }
}